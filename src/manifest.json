{
  "manifest_version": 3,
  "version": "0.0.1.23",
  "name": "Monk YouTube",
  "description": "Monk YouTube",
  "icons": {
    "128": "/img/logo.png"
  },
  "action": {
    "default_icon": "/img/logo.png",
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["https://www.youtube.com/*"],
      "run_at": "document_start",
      "js": ["/content.js"]
    }
  ],
  "permissions": ["storage"],
  "host_permissions": ["https://www.youtube.com/*"],
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["https://*/*", "http://*/*"],
      "extension_ids": []
    }
  ]
}
